% Define function and its find its derivative
syms x y
f2 = symfun(2*exp(-3*x), [x y]);
f = diff(f2,x);

% Solve ODE by rk2 method using mid point formula
[y2,t]=rk2_mid_point(f,0,0.2,10,2);

% Evalue ODE by rk4 method
[y4,t]=rk4(f,0,0.2,10,2);

% Evalue the original function
y=f2(t,zeros(size(t,1),1));

% Plot everything
hold on;
subplot(2,1,1);
plot(t,y);
plot(t,y2,'-.r');
subs(sqrt(sum((y - y2) .^ 2)))
hold on;
subplot(2,1,2);
plot(t,y);
plot(t,y4,'-.g');
subs(sqrt(sum((y - y4) .^ 2)))